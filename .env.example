# =============================================================================
# AI Multi-Agent Framework v4.0 - Environment Variables
# =============================================================================

# -----------------------------------------------------------------------------
# LLM API Keys
# -----------------------------------------------------------------------------
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# -----------------------------------------------------------------------------
# Search APIs
# -----------------------------------------------------------------------------
PERPLEXITY_API_KEY=your_perplexity_api_key_here
EXA_API_KEY=your_exa_api_key_here

# -----------------------------------------------------------------------------
# GitHub Synchronization
# -----------------------------------------------------------------------------
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_REPO=GilbertKwak/ai-multiagent-framework-v4
GITHUB_BRANCH=main
AUTO_COMMIT=true
COMMIT_INTERVAL=10  # 10회 실행마다 자동 커밋

# -----------------------------------------------------------------------------
# Deployment Platforms
# -----------------------------------------------------------------------------
# Together.ai
TOGETHER_API_KEY=your_together_api_key_here
TOGETHER_DEPLOYMENT_NAME=ai-multiagent-v4

# AWS
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=ai-multiagent-reports

# -----------------------------------------------------------------------------
# Monitoring & Observability
# -----------------------------------------------------------------------------
LANGSMITH_API_KEY=your_langsmith_api_key
LANGSMITH_PROJECT=ai-multiagent-v4
LANGSMITH_TRACING=true

PROMETHEUS_ENDPOINT=http://localhost:9090
GRAFANA_ENDPOINT=http://localhost:3000

# -----------------------------------------------------------------------------
# System Configuration
# -----------------------------------------------------------------------------
LOG_LEVEL=INFO
MAX_WORKERS=8
TIMEOUT_SECONDS=300
RETRY_ATTEMPTS=3

# Observational Memory
OBS_MEMORY_ENABLED=true
OBS_MEMORY_TOKEN_THRESHOLD=30000
OBS_MEMORY_BUFFER=0.2

# SkillRL
SKILLRL_ENABLED=true
SKILLRL_AUTO_EXTRACT=true
SKILLRL_LIBRARY_PATH=./data/skills

# SDFT
SDFT_ENABLED=true
SDFT_TEACHER_MODEL=gemini-2.5-pro
SDFT_STUDENT_MODEL=gemini-2.5-flash
SDFT_LEARNING_RATE=0.0001

# OpenEnv-Turing Benchmark
BENCHMARK_ENABLED=true
BENCHMARK_INTERVAL=10  # 매 10회 실행마다
BENCHMARK_AUTO_ROLLBACK=true
BENCHMARK_PASS_THRESHOLD=0.94

# -----------------------------------------------------------------------------
# Security
# -----------------------------------------------------------------------------
API_KEY_ENCRYPTION=true
SECRET_KEY=your_secret_encryption_key_here
CORS_ORIGINS=http://localhost:3000,https://your-domain.com

# -----------------------------------------------------------------------------
# Database
# -----------------------------------------------------------------------------
DATABASE_URL=postgresql://user:password@localhost:5432/ai_multiagent
REDIS_URL=redis://localhost:6379/0

# -----------------------------------------------------------------------------
# Feature Flags
# -----------------------------------------------------------------------------
EXA_PATTERN_ENABLED=true
TOKEN_OPTIMIZATION=true
PARALLEL_EXECUTION=true
AUTO_SCALING=true